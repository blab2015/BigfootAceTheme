<body class="{% block bodyClass %}{% endblock bodyClass %} skin-bigfoot">
{% block body %}
<div class="navbar navbar-default" id="navbar">
    {% block toolbar %}
        <div class="navbar-container" id="navbar-container">
            <div class="navbar-header pull-left">
                <a href="{{ path('admin_home') }}" class="navbar-brand">

                        <!-- NCH -->
                        {#{{ bigfoot_theme_values.title | trans }}#}
                        SEH - <small>Société Européenne d'Hôtellerie</small>
                        <!-- /NCH -->

                </a>
            </div>

            <div class="navbar-header pull-right" role="navigation">
                <!-- NCH -->
                <ul class="nav ace-nav">
                    <li class="brands grey">
                        <a href="#" id="brand-nav-toggler" class="btn btn-app no-radius dropdown-toggle brand-nav-toggler">
                            <i class="icon-folder-close"></i>
                            <i class="icon-folder-open"></i>
                            <span class="align-left">Marques</span>
                        </a>
                        <ul class="nav ace-nav brand-nav">

                            {{ render(controller('BigfootCoreBundle:QuickLink:quickLinkWidget')) }}
                            {#<li class="dropdown-header">#}
                                {#<i class="icon-folder-close-alt "></i>#}
                                {#Choisissez le groupe ou l'une de ses quatre marques hôtelières#}
                            {#</li>#}

                            <!-- NCH -->
                            <li class="brand pink">
                                <a class="nav-link" href="#">
                                    <span class="">SEH</span>
                                </a>
                            </li>

                            <li class="brand red active">
                                <a class="nav-link" href="#">
                                    <span class="">INTER-HOTEL</span>
                                </a>
                            </li>

                            <li class="brand yellow">
                                <a class="nav-link" href="#">
                                    <span class="">P'tit Dej-HOTEL</span>
                                </a>
                            </li>

                            <li class="brand brown">
                                <a class="nav-link" href="#">
                                    <span class="">QUALYS-HOTEL</span>
                                </a>
                            </li>

                            <li class="brand blue">
                                <a class="nav-link" href="#">
                                    <span class="">Relais du Silence</span>
                                </a>
                            </li>

                        </ul>
                    </li>
                    <li class="infos">
                        <ul class="nav ace-nav info-nav">

                            <li class="support">
                                <a href="#" class="align-right">
                                    <span class="align-left">Contactez le support</span>
                                    <i class="icon-sitemap align-center bigger-125 icon-on-right"></i>
                                </a>
                            </li>
                            <li class="account">
                                <a data-toggle="dropdown" href="#" class="dropdown-toggle align-right">
                                    <i class="icon-user"></i>
                                    <span class="user-info align-left">
                                        <small>Bienvenue,</small>
                                        Pat Hibulaire
                                    </span>
                                    <i class="icon-caret-down align-center icon-on-right"></i>
                                </a>

                                <ul class="user-menu pull-right dropdown-menu dropdown-yellow dropdown-caret dropdown-close">
                                    <li>
                                        <a href="#">
                                            <i class="icon-cog"></i>
                                            Settings
                                        </a>
                                    </li>

                                    <li>
                                        <a href="#">
                                            <i class="icon-user"></i>
                                            Profile
                                        </a>
                                    </li>

                                    <li class="divider"></li>

                                    <li>
                                        <a href="#">
                                            <i class="icon-off"></i>
                                            Logout
                                        </a>
                                    </li>
                                </ul>
                            </li>



                            {% if theme.toolbar.menu is defined and theme.toolbar.menu.items|length and app.security.token %}
                            <li class="account">
                                <a data-toggle="dropdown" href="#" class="dropdown-toggle">
                                    <i class="icon-user"></i>
                                    <span class="user-info">
                                        <small>{{ 'Welcome' | trans }},</small>
                                        {{ app.security.token.user.fullName }}
                                    </span>

                                    <i class="icon-caret-down"></i>
                                </a>
                                <ul class="user-menu pull-right dropdown-menu dropdown-yellow dropdown-caret dropdown-close">
                                    {% for item in theme.toolbar.menu.items %}
                                        <li><a href="{{ path(item.route, item.parameters) }}"><i class="icon-{{ item.icon }}"></i> {{ item.label | trans }}</a></li>
                                    {%  endfor %}
                                </ul>
                            </li>
                            {% endif %}

                        </ul>
                    </li>
                </ul>
                <!-- /NCH -->
            </div>
        </div>
    {% endblock toolbar %}
</div>
<div class="main-container" id="main-container">
    <div class="main-container-inner">
        <a class="menu-toggler" id="menu-toggler" href="#">
            <span class="menu-text"></span>
        </a>

        {% block sidebar -%}
            <div class="sidebar" id="sidebar">
                {{ knp_menu_render('main') }}
            </div>
        {%- endblock sidebar %}

        <div class="main-content">
        {% block main_content %}
            {% block header %}
            <div class="breadcrumbs" id="breadcrumbs">
                <ul class="breadcrumb">
                    <li>
                        <i class="icon-home home-icon"></i>
                        <a href="{{ path('admin_home') }}">{{ 'Home' | trans }}</a>
                    </li>
                    {% if breadcrumbs is defined %}
                        {% for item in breadcrumbs %}
                            <li{% if loop.last %} class="active"{% endif %}>
                                {% if not loop.last %}<a href="{{ item.url }}">{% endif %}{{ item.label | trans }}{% if not loop.last %}</a>{% endif %}
                            </li>
                        {% endfor %}
                    {% endif %}
                </ul>
                {{ render(controller('BigfootCoreBundle:QuickLink:quickLinkStar', {'currentRoute' : app.request.uri })) }}
            </div>
            {% endblock header %}

            <div class="page-content">
                {% block page_header %}
                <div class="page-header">
                    <h1>
                        {{ theme.page_header.title | default( bigfoot_theme_values.title ) | trans }}
                        {% if theme.page_header.subtitle is defined %}
                        <small>
                            <i class="icon-double-angle-right"></i>
                            {{ theme.page_header.subtitle | default('') | trans }}
                        </small>
                        {% endif %}
                    </h1>
                </div>
                {% endblock %}
                {% import 'BigfootAceThemeBundle:macros:flash.html.twig' as flash %}
                {% block flash_messages %}
                    {{ flash.messages('error') }}
                    {{ flash.messages('success') }}
                    {{ flash.messages('warning') }}
                    {{ flash.messages('notice') }}
                {% endblock flash_messages %}
                <div class="row">
                    <div class="col-xs-12">
                        {% block content %}
                            {% if dashboard is defined and dashboard|length > 0 %}
                                {% set row_width = 0 %}
                                <div class="row">
                                {% for widget in dashboard %}
                                    {% if (row_width + widget.width) > 12 %}
                                        {% set row_width = 0 %}
                                </div>
                                <div class="space-24"></div>
                                <div class="row">
                                    {% else %}
                                        {% set row_width = row_width + widget.width %}
                                    {% endif %}
                                {{ widget.render | raw}}
                                {% endfor %}
                                </div>
                            {% endif %}
                        {% endblock content %}
                    </div>
                </div>
            </div>
            {% endblock %}
        </div>
    </div>
    <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
        <i class="icon-double-angle-up icon-only bigger-110"></i>
    </a>
</div>

</div>
